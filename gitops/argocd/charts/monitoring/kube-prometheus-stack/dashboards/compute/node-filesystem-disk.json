              "left": "Used",
              "operator": "/",
              "reducer": "sum",
              "right": "Value #A (lastNotNull)"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "Value #A (lastNotNull)": "Size",
              "Value #B (lastNotNull)": "Available",
              "mountpoint": "Mounted on"
            }
          }
        }
      ],
      "type": "table"
